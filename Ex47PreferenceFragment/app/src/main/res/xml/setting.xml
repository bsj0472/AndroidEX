<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <SwitchPreference
        android:key="vibrate"
        android:title="진동알림"
        android:summary="메세지가 왔을때 진동울림을 알림으로 받으려면 체크하세요."/>

    <CheckBoxPreference
        android:key="message"
        android:title="메세지"
        android:summary="소리로 알림을 받으려면 체크하세요"/>

    <EditTextPreference
        android:key="nickname"
        android:title="닉네임"
        android:summary="닉네임을 설정하세요."
        android:icon="@mipmap/ic_launcher"/>

    <ListPreference
        android:key="sound"
        android:title="알림음"
        android:summary="알림을 설정하세요"
        android:entries="@array/datas"
        android:entryValues="@array/datas"/>





</PreferenceScreen>